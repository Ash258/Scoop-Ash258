{
    "version": "4.1.4",
    "description": "qBittorrent. Free and reliable P2P Bittorent client.",
    "homepage": "https://www.qbittorrent.org/",
    "license": {
        "identifier": "GPL-2.0-only",
        "url": "https://github.com/qbittorrent/qBittorrent/blob/master/COPYING"
    },
    "architecture": {
        "64bit": {
            "url": "https://downloads.sourceforge.net/project/qbittorrent/qbittorrent-win32/qbittorrent-4.1.4/qbittorrent_4.1.4_x64_setup.exe#/cosi.7z",
            "hash": "7aab70f038bb06d5107ef9444ea236b7b63ac7476b3491a5fc99da8354b14c38"
        },
        "32bit": {
            "url": "https://downloads.sourceforge.net/project/qbittorrent/qbittorrent-win32/qbittorrent-4.1.4/qbittorrent_4.1.4_setup.exe#/cosi.7z",
            "hash": "98321b4315fbbfa743ec71759703a56383d036fa9049cf94a2ec770fc019dad0"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\" -Force -Recurse",
    "persist": "profile",
    "bin": [
        [
            "qbittorrent.exe",
            "qBittorrent",
            "--portable"
        ]
    ],
    "shortcuts": [
        [
            "qbittorrent.exe",
            "qBittorrent",
            "--portable"
        ]
    ],
    "checkver": {
        "url": "https://www.qbittorrent.org/download.php",
        "regex": "Latest:\\s+v([\\d\\.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.sourceforge.net/project/qbittorrent/qbittorrent-win32/qbittorrent-$version/qbittorrent_$version_x64_setup.exe#/cosi.7z",
                "hash": {
                    "url": "https://www.qbittorrent.org/download.php",
                    "find": "64-bit.*\\s+.*<code>([A-Fa-f\\d]{40,128})</code>"
                }
            },
            "32bit": {
                "url": "https://downloads.sourceforge.net/project/qbittorrent/qbittorrent-win32/qbittorrent-$version/qbittorrent_$version_setup.exe#/cosi.7z",
                "hash": {
                    "url": "https://www.qbittorrent.org/download.php",
                    "find": "32-bit.*\\s+.*<code>([A-Fa-f\\d]{40,128})</code>"
                }
            }
        }
    }
}
